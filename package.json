    "coverage": "cross-env NODE_ENV=test nyc --include src --all -r text mocha"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "express": "^4.16.2",
    "mocha": "^5.0.3",
    "node-sass": "^4.7.2",
    "node-yaml-config": "^0.0.4",
    "nyc": "^11.4.1",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.1.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-persist": "^5.9.1",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^6.0.7",
    "socket.io": "^2.0.3"
